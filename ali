mark = int(input("enter student mark:"))
if (mark >= 90):
    grade = "A"
elif(mark >= 80 and mark < 90):
    grade = "B"
elif(mark >= 80 and mark < 70):
    grade = "C"
else:
    grade = "D"
    print("grade of student ->", grade)

