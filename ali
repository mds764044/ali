arithmetic operator
relation operator
a = 50
b = 20
print(a == b)
print(a != b)
print(a >= b)
print(a > b)
print(a <= b)
print(a < b)

assigment operator
num = 10
num *= 5
print("num:",num)

logical operator
print(not False)
print(not True)
val1 = True
val2 = False
print("AND operator:", val1 and val2)
print("OR operator:",val1 or val2)

